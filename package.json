{
  "name": "vendure-advanced-shipping",
  "description": "ðŸ“¦ A series of plugins for Vendure to add features to shipping using boxes, and shipping based on product dimensions.",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "watch": "wsrun watch",
    "build": "wsrun build",
    "test": "wsrun -m test",
    "lint": "wsrun -m lint",
    "lint:fix": "wsrun -m lint:fix",
    "codegen": "graphql-codegen --config gql-codegen.yml && wsrun -m codegen",
    "dev-server:populate": "node -r ts-node/register dev-server/populate.ts",
    "dev-server:start": "rimraf dev-server/admin-ui && concurrently npm:dev-server:run*",
    "dev-server:run:server": "node -r ts-node/register dev-server/index.ts",
    "dev-server:run:worker": "node -r ts-node/register dev-server/index-worker.ts",
    "release": "multi-semantic-release"
  },
  "dependencies": {
    "@vendure/admin-ui-plugin": "0.14.0",
    "@vendure/asset-server-plugin": "0.14.0",
    "@vendure/core": "0.14.0",
    "@vendure/create": "0.14.0",
    "@vendure/testing": "0.14.0",
    "@vendure/ui-devkit": "0.14.0"
  },
  "devDependencies": {
    "multi-semantic-release": "2.4.1",
    "@commitlint/config-conventional": "9.1.1",
    "@graphql-codegen/cli": "1.17.4",
    "@graphql-codegen/fragment-matcher": "1.17.4",
    "@graphql-codegen/introspection": "1.17.4",
    "@graphql-codegen/near-operation-file-preset": "1.17.4",
    "@graphql-codegen/schema-ast": "1.17.4",
    "@graphql-codegen/typescript": "1.17.4",
    "@graphql-codegen/typescript-operations": "1.17.4",
    "@types/jest": "26.0.5",
    "@types/node": "12.12.50",
    "@typescript-eslint/eslint-plugin": "3.6.1",
    "@typescript-eslint/parser": "3.6.1",
    "@vendure-advanced-shipping/core": "file:packages/core",
    "@vendure-advanced-shipping/melhor-envio": "file:packages/melhor-envio",
    "@vendure-advanced-shipping/rodonaves": "file:packages/rodonaves",
    "@vendure-advanced-shipping/ups-brazil": "file:packages/ups-brazil",
    "@vendure-advanced-shipping/pickup-in-store": "file:packages/pickup-in-store",
    "@vendure/admin-ui": "0.14.0",
    "codecov": "3.7.2",
    "commitlint": "9.1.0",
    "concurrently": "5.2.0",
    "eslint": "7.5.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.2.5",
    "jest": "26.1.0",
    "lerna": "3.22.1",
    "mysql": "2.18.1",
    "ncp": "2.0.0",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "sqlite3": "5.0.0",
    "ts-jest": "26.1.3",
    "ts-node": "8.10.2",
    "typescript": "3.9.7",
    "wsrun": "5.2.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E  HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        2,
        "never",
        [
          "start-case",
          "pascal-case"
        ]
      ]
    }
  },
  "workspaces": [
    "packages/*"
  ],
  "release": {
    "branches": [
      "master"
    ]
  }
}
